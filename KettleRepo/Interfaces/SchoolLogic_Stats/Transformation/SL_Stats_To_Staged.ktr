    <sql>select &#x27;Students currently active in the district&#x27; as cMessage, &#xd;&#xa;SUSER_name&#x28;&#x29; as cuser, &#xd;&#xa;getdate&#x28;&#x29; as ddate, &#xd;&#xa;count&#x28;&#x2a;&#x29; as iCount,&#xd;&#xa;&#x27;Enrollment&#x27; as cType&#xd;&#xa;from student&#xd;&#xa;left join studentstatus on studentstatus.istudentstatusid &#x3d; student.iActive_StudentStatusID &#xd;&#xa;left join LookupValues on iLookupValuesID &#x3d; studentstatus.iLV_InStatusValueID&#xd;&#xa;where student.lcurrent &#x3d; 1 and iLV_OutStatusValueID &#x3d; 0&#xd;&#xa;UNION&#xd;&#xa;select &#x27;Students currently active in &#x27;&#x2b;rtrim&#x28;School.cname&#x29; as cMessage, &#xd;&#xa;SUSER_name&#x28;&#x29; as cuser, &#xd;&#xa;getdate&#x28;&#x29; as ddate, &#xd;&#xa;count&#x28;&#x2a;&#x29; as iCount,&#xd;&#xa;&#x27;Enrollment&#x27; as cType&#xd;&#xa;from student&#xd;&#xa;left join studentstatus on studentstatus.istudentstatusid &#x3d; student.iActive_StudentStatusID &#xd;&#xa;left join LookupValues on iLookupValuesID &#x3d; studentstatus.iLV_InStatusValueID&#xd;&#xa;left join school on school.ischoolid &#x3d; studentstatus.ischoolid&#xd;&#xa;where student.lcurrent &#x3d; 1 and iLV_OutStatusValueID &#x3d; 0&#xd;&#xa;group by school.calternatecode, school.cname&#xd;&#xa;order by 1,3&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
