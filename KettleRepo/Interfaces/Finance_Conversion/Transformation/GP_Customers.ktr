  <step>
    <name>GP Customer Listing</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>GREATPLAINS_DB</connection>
    <sql>SELECT 	M.CUSTNMBR AS Old_Customer_ID,&#xd;&#xa;	ROW_NUMBER&#x28;&#x29; OVER &#x28;PARTITION BY M.CPRCSTNM ORDER BY M.CUSTNMBR&#x29; &#x2b; 10000 as Customer_ID,&#xd;&#xa;	M.CUSTNAME AS Customer_Name,&#xd;&#xa;	M.CUSTCLAS as Customer_Class,&#xd;&#xa;	A.CNTCPRSN AS Contact,&#xd;&#xa;	A.ADDRESS1 AS Address_1,&#xd;&#xa;	A.ADDRESS2 AS Address_2,&#xd;&#xa;	A.ADDRESS3 AS Address_3,&#xd;&#xa;	A.CITY AS City,&#xd;&#xa;	A.&#x5b;STATE&#x5d; AS Region,&#xd;&#xa;	A.ZIP AS PostCode,&#xd;&#xa;	A.COUNTRY AS Country,&#xd;&#xa;	CASE &#xd;&#xa;			WHEN A.PHONE1 &#x3d; &#x27;&#x27; OR LEFT&#x28;A.PHONE1,4&#x29; &#x3d; &#x27;0000&#x27; THEN &#x27;&#x27;&#xd;&#xa;			WHEN SUBSTRING&#x28;A.PHONE1,11,4&#x29; &#x3d; &#x27;0000&#x27; THEN &#x27;&#x28;&#x27;&#x2b;LEFT&#x28;A.PHONE1,3&#x29;&#x2b;&#x27;&#x29; &#x27;&#x2b;SUBSTRING&#x28;A.PHONE1,4,3&#x29;&#x2b;&#x27;-&#x27;&#x2b;SUBSTRING&#x28;A.PHONE1,7,4&#x29;&#xd;&#xa;			ELSE &#x27;&#x28;&#x27;&#x2b;LEFT&#x28;A.PHONE1,3&#x29;&#x2b;&#x27;&#x29; &#x27;&#x2b;SUBSTRING&#x28;A.PHONE1,4,3&#x29;&#x2b;&#x27;-&#x27;&#x2b;SUBSTRING&#x28;A.PHONE1,7,4&#x29;&#x2b;&#x27; &#x27;&#x2b;SUBSTRING&#x28;A.PHONE1,11,4&#x29; &#xd;&#xa;		END AS Phone_1,&#xd;&#xa;	CASE &#xd;&#xa;			WHEN A.PHONE2 &#x3d; &#x27;&#x27; OR LEFT&#x28;A.PHONE2,4&#x29; &#x3d; &#x27;0000&#x27; THEN &#x27;&#x27;&#xd;&#xa;			WHEN SUBSTRING&#x28;A.PHONE2,11,4&#x29; &#x3d; &#x27;0000&#x27; THEN &#x27;&#x28;&#x27;&#x2b;LEFT&#x28;A.PHONE2,3&#x29;&#x2b;&#x27;&#x29; &#x27;&#x2b;SUBSTRING&#x28;A.PHONE2,4,3&#x29;&#x2b;&#x27;-&#x27;&#x2b;SUBSTRING&#x28;A.PHONE2,7,4&#x29;&#xd;&#xa;			ELSE &#x27;&#x28;&#x27;&#x2b;LEFT&#x28;A.PHONE2,3&#x29;&#x2b;&#x27;&#x29; &#x27;&#x2b;SUBSTRING&#x28;A.PHONE2,4,3&#x29;&#x2b;&#x27;-&#x27;&#x2b;SUBSTRING&#x28;A.PHONE2,7,4&#x29;&#x2b;&#x27; &#x27;&#x2b;SUBSTRING&#x28;A.PHONE2,11,4&#x29; &#xd;&#xa;		END AS Phone_2,&#xd;&#xa;	CASE &#xd;&#xa;			WHEN A.PHONE3 &#x3d; &#x27;&#x27; OR LEFT&#x28;A.PHONE3,4&#x29; &#x3d; &#x27;0000&#x27; THEN &#x27;&#x27;&#xd;&#xa;			WHEN SUB