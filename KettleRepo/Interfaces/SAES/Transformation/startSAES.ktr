    <sql>Select distinct HR_EMPLOYEE_INTERLINK.employee_no,&#xd;&#xa;HR_EMPLOYEE_INTERLINK.last_name, &#xd;&#xa;HR_EMPLOYEE_INTERLINK.first_name,&#xd;&#xa;HR_EMPLOYEE_INTERLINK.middle_name,&#xd;&#xa;HR_EMPLOYEE_DEMOGRAPHICS.address_1, &#xd;&#xa;HR_EMPLOYEE_DEMOGRAPHICS.address_2, &#xd;&#xa;&#x27;&#x27; as address_3,&#xd;&#xa;&#x27;&#x27; as address_4,&#xd;&#xa;HR_EMPLOYEE_DEMOGRAPHICS.postal_code, &#xd;&#xa;HR_EMPLOYEE_DEMOGRAPHICS.city, &#xd;&#xa;HR_EMPLOYEE_DEMOGRAPHICS.province, &#xd;&#xa;HR_EMPLOYEE_DEMOGRAPHICS.country,&#xd;&#xa;concat&#x28;substring&#x28;HR_EMPLOYEE_DEMOGRAPHICS.phone_no,1,3&#x29;,&#x27;-&#x27;,substring&#x28;HR_EMPLOYEE_DEMOGRAPHICS.phone_no,4,3&#x29;,&#x27;-&#x27;,RIGHT&#x28;HR_EMPLOYEE_DEMOGRAPHICS.phone_no,4&#x29;&#x29; as phone_no,&#xd;&#xa;HR_EMAIL_ADDRESS.employee_email,&#xd;&#xa;&#x27;don.com&#x27; as emaildefault,&#xd;&#xa;&#x27;CERT&#x27; as Cert_type,&#xd;&#xa;HR_ASSIGNMENT_LOCATIONS.LOCATION,&#xd;&#xa;HR_ASSIGNMENTS.Hours_FTE,&#xd;&#xa;HR_ASSIGNMENTS.POSITION_CODE,&#xd;&#xa;substring&#x28;HR_EMPLOYEE_DEMOGRAPHICS.salary_grid,5,2&#x29; as SG_Education,&#xd;&#xa;substring&#x28;HR_EMPLOYEE_DEMOGRAPHICS.salary_grid,3,2&#x29; as SG_Experience&#xd;&#xa;from HR_EMPLOYEE_INTERLINK&#xd;&#xa;	left join HR_EMPLOYEE_DEMOGRAPHICS on HR_EMPLOYEE_DEMOGRAPHICS.employee_no &#x3d; HR_EMPLOYEE_INTERLINK.employee_no&#xd;&#xa;	left join HR_EMAIL_ADDRESS on HR_EMAIL_ADDRESS.EMPLOYEE_NO &#x3d; HR_EMPLOYEE_INTERLINK.employee_no&#xd;&#xa;	left join HR_ASSIGNMENTS on hr_assignments.EMPLOYEE_NO &#x3d; HR_EMPLOYEE_INTERLINK.EMPLOYEE_NO &#xd;&#xa;	left join HR_ASSIGNMENT_LOCATIONS on HR_ASSIGNMENT_LOCATIONS.THE_KEY &#x3d; HR_ASSIGNMENTS.THE_KEY&#xd;&#xa;	inner join HR_LOCATION on HR_LOCATION.LOCATION_NO &#x3d; HR_ASSIGNMENT_LOCATIONS.LOCATION&#xd;&#xa;where HR_EMPLOYEE_INTERLINK.status &#x3d; &#x27;A&#x27;&#xd;&#xa;    and HR_EMPLOYEE_DEMOGRAPHICS.BARGAINING_UNIT &#x3d; &#x27;ATA&#x27;&#xd;&#xa;	and &#x28;HR_ASSIGNMENTS.STATUS &#x3d; &#x27;A&#x27; OR  HR_ASSIGNMENTS.END_DATE &#x3d; &#x27;99999999&#x27;&#x29;&#xd;&#xa;order by last_name, first_name&#xd;&#xa;</sql>
